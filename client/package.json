{
    "name": "crane",
    "displayName": "Crane - PHP Intellisense",
    "description": "PHP Intellisense/code-completion",
    "author": {
        "name": "Hvy Industries",
        "url": "https://hvy.io",
        "email": "joe@hvy.io"
    },
    "icon": "images/icon.png",
    "license": "MIT",
    "version": "0.1.3",
    "publisher": "HvyIndustries",
    "engines": {
        "vscode": "0.10.x"
    },
    "bugs": {
        "url": "https://github.com/HvyIndustries/crane/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/HvyIndustries/crane.git"
    },
    "categories": [
        "Languages",
        "Other"
    ],
    "galleryBanner": {
        "theme": "dark",
        "color": "#232d33"
    },
    "private": true,
    "homepage": "https://hvy.io/crane",
    "activationEvents": [
        "onLanguage:php"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Example configuration",
            "properties": {
                "languageServerExample.maxNumberOfProblems": {
                    "type": "number",
                    "default": 100,
                    "description": "Controls the maximum number of problems produced by the server."
                }
            }
        },
        "keybindings": [
            {
                "key": "ctrl+d",
                "command": "crane.duplicateLine",
                "when": "editorTextFocus"
            }
        ],
        "commands": [
            {
                "command": "crane.reportBug",
                "title": "Crane - Report PHP Intellisense Bug"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
    },
    "devDependencies": {
        "typescript": "^1.6.2",
        "vscode": "0.10.x"
    },
    "dependencies": {
        "vscode-languageclient": "^1.1.0"
    }
}